generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

model User {
  email  String @id
  name   String
  course String
}

model Candidate {
  name   String @id
  course String
  post   String
  serial Int?
}

model Vote {
  id        String    @id @default(uuid())
  email     String
  post      String
  candidate String
  createdAt DateTime? @default(now())
}

model Course {
  name String @id
}
