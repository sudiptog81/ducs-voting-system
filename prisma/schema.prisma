generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

model User {
  email  String @id
  name   String
  course String
}

model Vote {
  email           String    @id
  president       String
  vice_president  String
  treasurer       String
  secretary       String
  joint_secretary String
  createdAt       DateTime? @default(now())
}
