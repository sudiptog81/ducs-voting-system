generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Vote {
  email           String    @id @db.VarChar(255)
  president       String    @db.VarChar(255)
  vice_president  String    @db.VarChar(255)
  treasurer       String    @db.VarChar(255)
  secretary       String    @db.VarChar(255)
  joint_secretary String    @db.VarChar(255)
  createdAt       DateTime? @default(now()) @db.Timestamp(0)
}
